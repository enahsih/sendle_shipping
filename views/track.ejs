    <!--tracking page begins here------------------------------>
     <!--Javascript code-->
        <%list.forEach(e =>{%>

       <div class="container-fluid tracker-info px-5  text-sm-center text-md-left text-lg-left mb-0">
        <!--First row tracking and other-->

            <div class="row trackingcode ">
                <div class="col-sm- col-md-6 col-lg-6 ">
                    <div class="text-sm-center text-md-left"><svg id="barcode" onload="generatebarcode('<%=e.Title%>')" style="margin-top: -30px;"></svg></div>
                 </div>
                <div class="col-sm- col-md-6  text-sm-center text-md-center text-lg-end " id="logo">
                    <span class="logo">SendleLogistics</span>
                </div>
            </div>

          
        <!--Second row---------------------------------------------------->
        <!--Receiver and shipper details------------------------------------------>

            <div class="row">
                <!--Shipper Address-->
                <div class="col-sm- col-md-6 col-lg-6  shipper-address">
                    <h4 class="header">Shipper Address</h4>
                    <div>
                        <%=e.Shipper%><br>
                        <%=e.ShPhone%><br>
                        <%=e.shAdd%><br>
                        <%=e.shmail%><br>
                    </div>
                </div>
                <!--Receiver address-->
                <div class="col-sm- col-md-6 col-lg-6">
                    <h4 class="header">Receiver Address</h4>
                    <div>
                        <%=e.Rec%><br>
                        <%=e.recPhone%><br>
                        <%=e.recAdd%><br>
                        <%=e.recmail%><br>
                    </div>
                </div>
            </div>

        <!--Third row-->
        <!--Shipment Details-------------------------------------------------------->
        <h4 class="headersd">Shipment Details</h4>    
        <div class="row">
                <!--First colum-->
                <div class="col-sm- col-md-6 col-lg-4">
                    <div>
                        <h5>Origin</h5>
                        <p><%=e.Origin%></p>
                    </div>
                    <div>
                        <h5>Destination</h5>
                        <p><%=e.Destination%></p>
                    </div>
                    <div>
                        <h5>Weight</h5>
                        <p><%=e.Weight%></p>
                    </div>
                    <div>
                        <h5>Product</h5>
                        <p><%=e.Product%></p>
                    </div>
                    <div>
                        <h5>Total Freight</h5>
                        <p><%=e.Total_Freight%></p>
                    </div>
                    <div>
                        <h5>Pickup Date</h5>
                        <p><%=e.POD%></p>
                    </div>
                    <div>
                        <h5>Comments</h5>
                        <p><%=e.Comments%></p>
                    </div>
                </div>
        <!--Second column--------------------------------------------------------->
                <div class="col-sm- col-md-6 col-lg-4">
                    <div>
                        <h5>Package</h5>
                        <p><%=e.Packages%></p>
                    </div>
                    <div>
                        <h5>Carrier</h5>
                        <p><%=e.Carrier%></p>
                    </div>
                    <div>
                        <h5>Shipment Mode</h5>
                        <p><%=e.Mode%></p>
                    </div>
                    <div>
                        <h5>Qty</h5>
                        <p><%=e.Quantity%></p>
                    </div>
                    <div>
                        <h5>Expected Delivery Date</h5>
                        <p><%=e.EDD%></p>
                    </div>
                    <div>
                        <h5>Pickup time</h5>
                        <p><%=e.Pickup_time%></p>
                    </div>
                </div>
                <!--last column-->
                <div class="col-sm- col-md-6 col-lg-4">
                    <div>
                        <h5>Carrier Reference No.</h5>
                        <p><%=e.CRN%></p>
                    </div>
                    <div>
                        <h5>Payment Mode</h5>
                        <p><%=e.Payment%></p>
                    </div>
                    <div>
                        <h5>Departure Time</h5>
                        <p><%=e.Departure_time%></p>
                    </div>
                </div>
                
            </div>

    <!--Fourth row------------------------------------------------->
    
            
                <div class="row shipmenthistory">
                    <div class="container-fluid gx-5">
                        <h4 class="header">Shipment History</h4>
                        <table class="table  table-bordered table-striped">
                            <thead></thead>
                            <tbody>
                                <tr>
                                    <td>
                                        <%list2.forEach(e=>{%>
                                            <div class="items">
                                                <p><span>Date:</span>  <%=e.Date%></p>
                                                <p><span>Time:</span>  <%=e.Time%></p>
                                                <p><span>Location:</span>  <%=e.Location%></p>
                                                <p><span>Status:</span>  <span style="background-color: yellow;"><%=e.Status%></span></p>
                                                <p><span>Updated By:</span>  <%=e.Updated_by%></p>
                                                <p><span>Remarks:</span>  <%=e.Remarks%></p>
    
                                            </div>
    
                                        <%})%>
                                    </td>
        
                                </tr>
    
                            </tbody>
                            <tfoot></tfoot>
    
                        </table>

                    </div>
                   

                </div>
                
    

        </div>
     
        <%})%>
